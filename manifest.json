{
  "description": "Simple tab manager.",
  "manifest_version": 2,
  "name": "OverTab",
  "version": "3.0",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": ["tabs", "contextualIdentities", "contextMenus"]
}